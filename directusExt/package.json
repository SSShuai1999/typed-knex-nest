{
  "name": "directus-ext",
  "version": "0.0.1",
  "description": "directus 扩展",
  "author": "SSShuai1999",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "zx:start": "npx zx script/build.mjs && npm run build:rollup",
    "zx:move": "npx zx script/move.mjs",
    "zx:install": "npx zx script/install.mjs",
    "zx:rm": "npx zx script/rm.mjs",
    "g:m": "npx zx script/genderModuleByName.mjs",
    "g:m2": "npx zx script/genderModuleByNameNoSpec.mjs",
    "to:zod": "npx zx script/toZod.mjs",
    "build:rollup": "rollup --config",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@vscode/sqlite3": "^5.0.8",
    "@wwwouter/typed-knex": "^4.8.2",
    "class-transformer": "^0.5.1",
    "inquirer": "^8.2.2",
    "knex": "^1.0.5",
    "mysql": "^2.18.1",
    "nestjs-knex": "^2.0.0",
    "nestjs-modules": "^1.0.9",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "ts-to-zod": "^1.10.0",
    "zod": "^3.14.4",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.1",
    "@types/lodash": "^4.14.182",
    "chalk": "^5.0.1",
    "fs-extra": "^10.1.0",
    "globby": "^13.1.1",
    "ignore": "^5.2.0",
    "minimist": "^1.2.6",
    "nestjs": "^0.0.1",
    "node-fetch": "^3.2.5",
    "ps-tree": "^1.2.0",
    "which": "^2.0.2",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "9.8.0",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "rollup": "^2.70.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
